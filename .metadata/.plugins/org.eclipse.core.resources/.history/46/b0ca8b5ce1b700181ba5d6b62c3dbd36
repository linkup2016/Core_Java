public class Test {

	public static void main(String[] args) {
		int i = 0;
		int sv = 0;
		
		Scanner input = new Scanner(System.in);
		ArrayList<Student> studentList = new ArrayList();
		while (sv != -1) {
			Student student1 = new Student();
			System.out.println("Student ID: ");			
